{
  "name": "obs-multistream",
  "version": "1.0.0",
  "description": "A multistream application for OBS to broadcast to multiple platforms simultaneously",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "docker:build": "docker build -t obs-multistream .",
    "docker:run": "docker run -p 1935:1935 -p 3000:3000 obs-multistream",
    "rasa:train": "cd rasa && rasa train",
    "rasa:run": "cd rasa && rasa run --enable-api --cors '*'",
    "rasa:shell": "cd rasa && rasa shell",
    "setup:rasa": "pip install rasa && rasa init --no-prompt --init-dir rasa"
  },
  "keywords": [
    "obs",
    "multistream",
    "rtmp",
    "streaming",
    "broadcast",
    "twitch",
    "youtube",
    "facebook",
    "instagram",
    "tiktok",
    "linkedin",
    "twitter",
    "discord",
    "telegram",
    "chatbot",
    "rasa",
    "ai",
    "social-media"
  ],
  "author": "StreaminDoDo Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "node-media-server": "^2.6.8",
    "ws": "^8.14.2",
    "fluent-ffmpeg": "^2.1.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "twitter-api-v2": "^1.15.2",
    "discord.js": "^14.14.1",
    "telegraf": "^4.15.0",
    "instagram-private-api": "^1.45.3",
    "tiktok-scraper": "^1.4.36",
    "node-fetch": "^3.3.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.2",
    "redis": "^4.6.11",
    "bull": "^4.12.2",
    "rate-limiter-flexible": "^2.4.1",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}